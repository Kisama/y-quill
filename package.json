{
  "name": "y-quill",
  "version": "0.0.1",
  "description": "QuillJs bindings for Yjs",
  "main": "./dist/y-quill.js",
  "module": "./src/y-quill.js",
  "sideEffects": false,
  "scripts": {
    "dist": "rm -rf dist && rollup -c",
    "test": "rollup -c && node test/test.node.js",
    "lint": "standard && tsc",
    "watch": "rollup -wc",
    "debug": "concurrently 'live-server --port=3443 --entry-file=test.html' 'npm run watch'",
    "preversion": "npm run lint && npm run dist && npm run test"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/y-js/y-quill.git"
  },
  "keywords": [
    "Yjs"
  ],
  "author": "Kevin Jahns <kevin.jahns@protonmail.com>",
  "license": "MIT",
  "standard": {
    "ignore": [
      "/dist",
      "/node_modules",
      "/docs"
    ]
  },
  "bugs": {
    "url": "https://github.com/y-js/y-quill/issues"
  },
  "homepage": "https://github.com/y-js/y-quill#readme",
  "dependencies": {
    "lib0": "0.0.5"
  },
  "peerDependencies": {
    "yjs": ">=13.0.0-89",
    "y-protocols": ">=0.0.6",
    "quill": "^1.3.6"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "jsdom": "^15.0.0",
    "live-server": "^1.2.1",
    "quill": "^1.3.6",
    "rollup": "^1.10.1",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.3",
    "standard": "^12.0.1",
    "y-protocols": "0.0.6",
    "yjs": "13.0.0-89"
  }
}
